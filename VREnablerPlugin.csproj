<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net35</TargetFramework>
    <FrameworkPathOverride Condition="'$(TargetFramework)' == 'net35'">$(MSBuildProgramFiles32)\Reference Assemblies\Microsoft\Framework\.NETFramework\v3.5\Profile\Client</FrameworkPathOverride>
    <AutomaticallyUseReferenceAssemblyPackages Condition=" '$(TargetFramework)' == 'net35' ">false</AutomaticallyUseReferenceAssemblyPackages>
  </PropertyGroup>

  <ItemGroup>
    <!-- Melon References in VREnablerPlugin.csproj.user -->
    <Reference Include="MelonLoader">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(MelonDir)\MelonLoader.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AssetsTools.NET" Version="2.0.9">
      <SpecificVersion>False</SpecificVersion>
    </PackageReference>
  </ItemGroup>
  
  
</Project>
